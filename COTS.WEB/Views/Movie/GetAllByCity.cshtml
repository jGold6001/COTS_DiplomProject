@model IEnumerable<COTS.WEB.Models.MovieViewModel>

@{
    ViewBag.Title = "GetAllByCity";
}


    @foreach (var item in Model)
    {
        <div class="container">          
            <div class="row" >
                <a href="http://@Request.Url.Authority/movie/@ViewData["cityId"]/@item.Id">@item.Name</a>
            </div>
        </div>
    }



